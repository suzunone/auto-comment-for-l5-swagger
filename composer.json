{
  "name": "suzunone/l-auto-comment-for-php-swagger",
  "description": "Automatically generate PHP Swagger comments from Laravel's Controller, Model, and form requests.",
  "type": "library",
  "require": {
    "barryvdh/laravel-ide-helper": "^2.11",
    "ext-mbstring": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "darkaonline/l5-swagger": "^8.1"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "AutoCommentForPHPSwagger\\": "src/"
    }
  },
  "authors": [
    {
      "name": "suzunone",
      "email": "suzunone.eleven@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "AutoCommentForPHPSwagger\\LAutoCommentForPHPSwaggerServiceProvider"
      ]
    }
  }
}
